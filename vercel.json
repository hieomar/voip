{
    "version": 2,
    "builds": [
        {
            "src": "index.php",
            "use": "vercel-php"
        }
    ],
    "routes": [
        {
            "src": "/images/(.*)",
            "dest": "/images/(.*)"
        },
        {
            "src": "/styles.css",
            "dest": "/styles.css"
        },
        {
            "src": "/content.pdf",
            "dest": "/content.pdf"
        },
        {
            "src": "/(.*)",
            "dest": "index.php"
        },
        {
            "src": "/(.*)",
            "dest": "/about.php"
        },
        {
            "src": "/company.php",
            "dest": "/company.php"
        },
        {
            "src": "/demo.php",
            "dest": "/demo.php"
        },
        {
            "src": "/faq.php",
            "dest": "/faq.php"
        },
        {
            "src": "/lang.php",
            "dest": "/lang.php"
        },
        {
            "src": "/partner-program.php",
            "dest": "/partner-program.php"
        },
        {
            "src": "/professional-services.php",
            "dest": "/professional-services.php"
        },
        {
            "src": "/resources.php",
            "dest": "/resources.php"
        },
        {
            "src": "/saas.php",
            "dest": "/saas.php"
        },
        {
            "src": "/solutions.php",
            "dest": "/solutions.php"
        },
        {
            "src": "/Untitled-1.php",
            "dest": "/Untitled-1.php"
        }
    ]
}