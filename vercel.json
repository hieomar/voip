{
    "version": 2,
    "builds": [
        {
            "src": "index.php",
            "use": "vercel-php"
        }
    ],
    "routes": [
        {
            "src": "/images/(.*)",
            "dest": "/api/images/$1"
        },
        {
            "src": "/styles.css",
            "dest": "/api/styles.css"
        },
        {
            "src": "/(.*)",
            "dest": "index.php"
        }
    ]
}