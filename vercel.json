{
    "version": 2,
    "builds": [
        {
            "src": "index.php",
            "use": "vercel-php"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.php"
        },
        {
            "src": "/solutions.php",
            "dest": "solutions.php"
        }
    ]
}