{
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
        {
            "src": "/favicon.ico",
            "dest": "/api/favicon.ico"
        },
        {
            "src": "/images/(.*)",
            "dest": "/api/images/$1"
        },
        {
            "src": "/styles.css",
            "dest": "/api/styles.css"
        },
        {
            "src": "/(.*)",
            "dest": "/api/$1.php"
        },
        {
            "src": "/",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "NOW_PHP_DEBUG": "1"
    }
}